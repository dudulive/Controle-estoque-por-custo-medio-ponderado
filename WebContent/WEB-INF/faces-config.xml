<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>actionVoltarMenu</from-outcome>
			<to-view-id>/publico/principal.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>publico/principal.xhtml</display-name>
		<from-view-id>/publico/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/lista_produto.xhtml</display-name>
		<from-view-id>/produto/lista_produto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionManterProduto</from-outcome>
			<to-view-id>/produto/form_produto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/produtoSucesso.xhtml</display-name>
		<from-view-id>/produto/produtoSucesso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarAlteracaoProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/form_produto.xhtml</display-name>
		<from-view-id>/produto/form_produto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>publico/principal.xhtml</display-name>
		<from-view-id>/publico/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarEntrada</from-outcome>
			<to-view-id>/entrada/lista_entrada.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>entrada/lista_entrada.xhtml</display-name>
		<from-view-id>/entrada/lista_entrada.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionManterEntrada</from-outcome>
			<to-view-id>/entrada/form_entrada.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>entrada/form_entrada.xhtml</display-name>
		<from-view-id>/entrada/form_entrada.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarEntrada</from-outcome>
			<to-view-id>/entrada/lista_entrada.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>entrada/entradaSucesso.xhtml</display-name>
		<from-view-id>/entrada/entradaSucesso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarAlteracaoEntrada</from-outcome>
			<to-view-id>/entrada/lista_entrada.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>publico/principal.xhtml</display-name>
		<from-view-id>/publico/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarSaida</from-outcome>
			<to-view-id>/saida/lista_saida.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>saida/lista_saida.xhtml</display-name>
		<from-view-id>/saida/lista_saida.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionManterSaida</from-outcome>
			<to-view-id>/saida/form_saida.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>saida/form_saida.xhtml</display-name>
		<from-view-id>/saida/form_saida.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarSaida</from-outcome>
			<to-view-id>/saida/lista_saida.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>saida/saidaSucesso.xhtml</display-name>
		<from-view-id>/saida/saidaSucesso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarAlteracaoSaida</from-outcome>
			<to-view-id>/saida/lista_saida.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>publico/principal.xhtml</display-name>
		<from-view-id>/publico/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarSaldo</from-outcome>
			<to-view-id>/saldo/lista_saldo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>saldo/lista_saldo_produto.xhtml</display-name>
		<from-view-id>/saldo/lista_saldo_produto.xhtml</from-view-id>
		<navigation-case>
		    <from-outcome>actionListarSaldo</from-outcome>
			<to-view-id>/saldo/lista_saldo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>pt_BR</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>org.projetoIntegrador.messages.messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>

</faces-config>
